.tabs {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 8vh;
  display: flex;
  text-align: center;
  font-size: 24rpx;
  color: #928155;
  z-index: 101;
  border-top: 1rpx solid #aaa;
  background-color: #fff; }
  .tabs .tab-item {
    flex: 1;
    display: flex;
    border-right: 1rpx solid #aaa;
    align-items: center;
    justify-content: center; }
    .tabs .tab-item:last-child{
      border-right: 0;
    }
    .tabs .tab-item .tab-box {
      width: 100%;
       height: 100%;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center; 
      }
      .tabs .tab-item .tab-box.active {
      background: #f3f3f3 }
      .tabs .tab-item .tab-box .icon {
        position: relative;
        display: inline-block;
        width: 50rpx;
        /*default*/
        height: 50rpx;
        /*default*/
        background-size: contain;
        background-repeat: no-repeat;
        background-position: center;
        vertical-align: middle; }
      .tabs .tab-item .tab-box .icon-program {
        background-image: url(data:image/png;base64,); }
      .tabs .tab-item .tab-box .icon-collection {
        background-image: url(data:image/png;base64,); }
       .tabs .tab-item .tab-box .icon-roadshow {
        background-image: url(data:image/png;base64,); }
      .tabs .tab-item .tab-box .icon-ensong {
        background-image: url(data:image/png;base64,); }
      .tabs .tab-item .tab-box.active .icon-program-active {
        background-image: url(data:image/png;base64,); }
      .tabs .tab-item .tab-box.active .icon-collection-active {
        background-image: url(data:image/png;base64,); }
      .tabs .tab-item .tab-box.active .icon-roadshow-active {
        background-image: url(data:image/png;base64,); }
      .tabs .tab-item .tab-box.active .icon-ensong-active {
        background-image: url(data:image/png;base64,); }
      .down{
        background-image: url('../../src/images/icon/download.png');
      }
     .collect{
       background-image: url('../../src/images/icon/redheart.png')
     }
